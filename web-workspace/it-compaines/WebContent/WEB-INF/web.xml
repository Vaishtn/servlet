<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
	id="WebApp_ID" version="4.0">
	<display-name>it-compaines</display-name>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
			</welcome-file-list>
	<servlet>
		<servlet-name>companyweb</servlet-name>
		<servlet-class>com.xworkz.it.web.CompanyWeb</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
	
	<servlet-mapping>
	<servlet-name>companyweb</servlet-name>
	<url-pattern>/CompanyView</url-pattern>
	</servlet-mapping>
	
	<servlet>
	<servlet-name>redChillyServelet</servlet-name>
	<servlet-class>com.xworkz.it.web.RedChilliServlet</servlet-class>
	<load-on-startup>2</load-on-startup>
	</servlet>
</web-app>